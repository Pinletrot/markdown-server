        "vue": "^2.0.1"
    },
    "devDependencies": {
        "babel-core": "^6.0.0",
        "babel-preset-es2015": "^6.0.0",
        "babelify": "^7.2.0",
        "browserify": "^13.0.1",
        "browserify-hmr": "^0.3.1",
        "cross-env": "^1.0.6",
        "envify": "^3.4.1",
        "uglify-js": "^2.5.0",
        "vueify": "^9.1.0",
        "watchify": "^3.4.0"
    },
    "browserify": {
        "transform": [
            "vueify",
            "babelify"
        ]
    },
    "browser": {
        "vue": "vue/dist/vue.common.js"
    }
